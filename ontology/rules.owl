    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#entryIntoForce -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#entryIntoForce">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#LawText"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>
    


