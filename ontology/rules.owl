    <!-- http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#DeonticModality -->

    <owl:Class rdf:about="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#DeonticModality">
        <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#Modality"/>
    </owl:Class>
    


    <!-- http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#FactualStatement -->

    <owl:Class rdf:about="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#FactualStatement">
        <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#LogicalFormula"/>
    </owl:Class>
    


    <!-- http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#LegalRuleMLDocument -->

    <owl:Class rdf:about="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#LegalRuleMLDocument">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#RuleBaseDocument"/>
    </owl:Class>
    


    <!-- http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#LogicalFormula -->

    <owl:Class rdf:about="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#LogicalFormula">
        <rdfs:label xml:lang="en">Logical Formua</rdfs:label>
        <rdfs:label xml:lang="en">Statement</rdfs:label>
    </owl:Class>
    


    <!-- http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#Modality -->

    <owl:Class rdf:about="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#Modality"/>
    


    <!-- http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#Sources -->

    <owl:Class rdf:about="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#Sources"/>
    


    <!-- http://ns.inria.fr/nrv#Compliance -->

    <owl:Class rdf:about="http://ns.inria.fr/nrv#Compliance">
        <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#FactualStatement"/>
        <rdfs:label>ComplianceStatement</rdfs:label>
    </owl:Class>
    


    <!-- http://ns.inria.fr/nrv#CompliantRequirement -->

    <owl:Class rdf:about="http://ns.inria.fr/nrv#CompliantRequirement">
        <rdfs:subClassOf rdf:resource="http://ns.inria.fr/nrv#ViolableRequirement"/>
        <owl:disjointWith rdf:resource="http://ns.inria.fr/nrv#ViolatedRequirement"/>
    </owl:Class>
    


    <!-- http://ns.inria.fr/nrv#NonViolableRequirement -->

    <owl:Class rdf:about="http://ns.inria.fr/nrv#NonViolableRequirement">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#RegulativeStatement"/>
    </owl:Class>
    


    <!-- http://ns.inria.fr/nrv#ViolableRequirement -->

    <owl:Class rdf:about="http://ns.inria.fr/nrv#ViolableRequirement">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#RegulativeStatement"/>
    </owl:Class>
    


    <!-- http://ns.inria.fr/nrv#ViolatedRequirement -->

    <owl:Class rdf:about="http://ns.inria.fr/nrv#ViolatedRequirement">
        <rdfs:subClassOf rdf:resource="http://ns.inria.fr/nrv#ViolableRequirement"/>
    </owl:Class>
    


    <!-- http://ns.inria.fr/nrv#Violation -->

    <owl:Class rdf:about="http://ns.inria.fr/nrv#Violation">
        <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#FactualStatement"/>
        <rdfs:label>ViolationStatement</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#Alinea -->

    <owl:Class rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#Alinea">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#DocumentElement"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#Applicability -->

    <owl:Class rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#Applicability">
        <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#FactualStatement"/>
        <rdfs:label>ApplicabilityStatement</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#Article -->

    <owl:Class rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#Article">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#DocumentElement"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#Chapter -->

    <owl:Class rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#Chapter">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#DocumentElement"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#Clause -->

    <owl:Class rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#Clause">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#DocumentElement"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#ConstitutiveStatement -->

    <owl:Class rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#ConstitutiveStatement">
        <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#LogicalFormula"/>
        <rdfs:comment>Definition of concepts and ideas in a regulation</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#Decision -->

    <owl:Class rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#Decision">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#Legislation"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#Directive -->

    <owl:Class rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#Directive">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#Legislation"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#Division -->

    <owl:Class rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#Division">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#DocumentElement"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#Document -->

    <owl:Class rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#Document"/>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#DocumentElement -->

    <owl:Class rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#DocumentElement">
        <rdfs:comment>class for any part of a larger documents. Its sub-classes have been nostly extracted from the Akoma Ntoso schema</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#ExceptionStatement -->

    <owl:Class rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#ExceptionStatement">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#RegulativeStatement"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#FacultativeModality -->

    <owl:Class rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#FacultativeModality">
        <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#DeonticModality"/>
        <rdfs:comment>Negation of an Obligation Modality</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#FacultativeStatement -->

    <owl:Class rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#FacultativeStatement">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#RegulativeStatement"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#containsModality"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#FacultativeModality"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://ns.inria.fr/nrv#NonViolableRequirement"/>
