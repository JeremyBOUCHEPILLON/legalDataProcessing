<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/jbouchep/leads/rules#"
     xml:base="http://www.semanticweb.org/jbouchep/leads/rules"
     xmlns:adr="https://spec.edmcouncil.org/fibo/ontology/FND/Places/Addresses#"
     xmlns:nrv="http://ns.inria.fr/nrv#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:lrml="http://docs.oasis-open.org/legalruleml/ns/v1.0#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:lrmlmm="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#"
     xmlns:rulemm="http://docs.oasis-open.org/legalruleml/ns/v1.0/rule-metamodel#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:sparql-service-description="http://www.w3.org/ns/sparql-service-description#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/jbouchep/leads/rules">
        <dcterms:license>http://creativecommons.org/licenses/by/3.0/</dcterms:license>
    </owl:Ontology>
    


