    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2006/time#generalDay -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2006/time#generalDay"/>
    


    <!-- http://www.w3.org/2006/time#generalMonth -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2006/time#generalMonth"/>
    


    <!-- http://www.w3.org/2006/time#generalYear -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2006/time#generalYear"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#ElemContainsElem -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#ElemContainsElem">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#DocumentElement"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#DocumentElement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#containsElem -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#containsElem">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#isPartOfDoc"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#Document"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#DocumentElement"/>
        <rdfs:comment>Indicates what DocumentElement are in a Document.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#containsModality -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#containsModality">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#RegulativeStatement"/>
        <rdfs:range rdf:resource="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#DeonticModality"/>
        <rdfs:comment>Indication that a statement contains a certain modality</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#formalizationOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#formalizationOf">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#formalizedBy"/>
        <rdfs:domain rdf:resource="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#LogicalFormula"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#DocumentElement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#formalizedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#formalizedBy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#DocumentElement"/>
        <rdfs:range rdf:resource="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#LogicalFormula"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#hasApplicableCode -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#hasApplicableCode">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#isApplicableToOffence"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#Offence"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#LawText"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#hasApplicableRule -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#hasApplicableRule">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#isApplicable"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#SituationDescriptionStatement"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#RegulativeStatement"/>
        <rdfs:comment>Indicates a rule (regulative statement) applicable to a situation (factual statement).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#hasCompliance -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#hasCompliance">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#isCompliantWith"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#RegulativeStatement"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#SituationDescriptionStatement"/>
        <owl:propertyDisjointWith rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#isViolatedBy"/>
        <rdfs:comment>Indicates the situations that comply with a rule</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#hasException -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#hasException">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#isExceptionOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#RegulativeStatement"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#ExceptionStatement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#hasSource -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#hasSource">
        <rdfs:domain rdf:resource="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#Sources"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#LawText"/>
        <rdfs:comment>inspired by lrmlmm:hasLegalSource</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#hasSources -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#hasSources">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#RuleBaseDocument"/>
        <rdfs:range rdf:resource="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#Sources"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#hasStatement -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#hasStatement">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#LogicalFormulas"/>
        <rdfs:range rdf:resource="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#LogicalFormula"/>
        <rdfs:label>hasLogicalFormula</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#hasStatements -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#hasStatements">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#RuleBaseDocument"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#LogicalFormulas"/>
        <rdfs:label>hasLogicalFormulas</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#inLanguage -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#inLanguage">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#Document"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#Language"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#isApplicable -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#isApplicable">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#RegulativeStatement"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#SituationDescriptionStatement"/>
        <rdfs:comment>Indicates which situation (factual statement) a rule (regulative statement) can be applied to.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#isApplicableToOffence -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#isApplicableToOffence">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#LawText"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#Offence"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#isCompliantWith -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#isCompliantWith">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#SituationDescriptionStatement"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#RegulativeStatement"/>
        <rdfs:comment>Indicates what rules a situation comply to.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#isExceptionOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#isExceptionOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#ExceptionStatement"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#RegulativeStatement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#isPartOfDoc -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#isPartOfDoc">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#DocumentElement"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#Document"/>
        <rdfs:comment>Indicates the document a DocumentElement is part of</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#isViolatedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#isViolatedBy">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#violates"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#RegulativeStatement"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#SituationDescriptionStatement"/>
        <rdfs:comment>Indicates the situations that violate a rule</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jbouchep/leads/rules#violates -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jbouchep/leads/rules#violates">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#SituationDescriptionStatement"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jbouchep/leads/rules#RegulativeStatement"/>
        <rdfs:comment>Indicates which rules a situation violates.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topObjectProperty -->

    <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#topObjectProperty">
        <rdfs:domain rdf:resource="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#LegalRuleMLDocument"/>
        <rdfs:range rdf:resource="http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel#Sources"/>
    </rdf:Description>
    


